[
  {
    "name"              : "dayswithouthacks.lol",
    "script"            : "yarn",
    "args"              : "start",
    "log_date_format"   : "YYYY-MM-DD HH:mm Z",
    "merge_logs"        : false,
    "watch"             : false,
    "max_restarts"      : 10,
    "exec_mode"         : "fork_mode",
    "env"               : {
      "DEBUG": "srv:*",
      "DEBUG_COLORS": true,
      "TELEGRAM_BOT_TOKEN": "<bot token>",
      "TELEGRAM_CHAT_ID": "<chat id>",
      "NODE_ENV": "production"
    }
  }
]